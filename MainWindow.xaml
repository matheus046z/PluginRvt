<Window x:Class="PluginRvt.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Inserir FURO-QUADRADO-LAJE" Height="200" Width="300">

    <StackPanel Margin="10">

        <TextBlock Text="Arquivo de Tubulação:" />
        <ComboBox x:Name="cbTubos" Margin="0,5"/>

        <TextBlock Text="Arquivo Estrutural:"/>
        <ComboBox x:Name="cbEstrutura" Margin="0,5"/>

        <Button Content="Furar" Margin="0,15,0,0" Click="Furar_Click"/>
    </StackPanel>
</Window>

